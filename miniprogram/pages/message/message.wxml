<view class="page"> 
    <view class="page__bd">
        <view class="weui-panel weui-panel_access">
            <view class="weui-panel__bd" >
                <navigator url="/pages/travelDetail/travelDetail?id={{userid}}" class="weui-media-box weui-media-box_appmsg" hover-class="weui-cell_active"  wx:for="{{queryResult}}">
                    <view class="weui-media-box__bd weui-media-box__bd_in-appmsg">
                        <view class="weui-media-box__title" >{{item.time}}</view>
                        <view class="weui-media-box__desc" >去往{{item.dest}}的{{item.content}}</view>
                    </view>
                </navigator>
            </view>
        </view>    
    </view>
</view>